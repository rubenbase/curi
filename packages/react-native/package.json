    "babel-jest": "^23.4.2",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^23.4.2",
    "react": "^16.7.0-alpha.0",
    "react-native": "^0.52.0",
    "react-test-renderer": "^16.7.0-alpha.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "ts-jest": "^23.10.4"
